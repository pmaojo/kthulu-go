    "install:deps": "playwright install-deps"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "zod": "^4.0.14"
  }
}