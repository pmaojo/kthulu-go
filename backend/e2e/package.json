{
  "name": "kthulu-e2e",
  "version": "1.0.0",
  "description": "End-to-end tests for Kthulu application",
  "private": true,
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
